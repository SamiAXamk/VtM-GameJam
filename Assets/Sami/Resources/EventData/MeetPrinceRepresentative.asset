    isItalic: 0
    hasReward: 0
    rewardType: 0
    rewardSkill: 0
    rewardValue: 0
    choices: []
    playerCharacter: 1
    rightSideCharacter: {fileID: 21300000, guid: 81f87ecf2f063664fbf357c339799d5e, type: 3}
